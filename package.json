{
  "name": "uniapp-demo",
  "version": "1.0.0",
  "description": "uniapp-demo",
  "main": "main.js",
  "scripts": {
    "prepare": "husky install",
    "commit": "git-cz",
    "commit:all": "git add . && git-cz"
  },
  "dependencies": {
    "uview-ui": "^1.8.8"
  },
  "devDependencies": {
    "commitizen": "^4.3.0",
    "commitlint": "^17.4.4",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^7.0.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0"
  },
  "keywords": [
    "uniapp",
    "anyup",
    "demo"
  ],
  "author": "anyup",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "*.{js,json}": [
      "eslint --fix"
    ]
  },
  "uni-app": {
    "scripts": {
      "h5-custom": {
        "title": "自定义H5平台",
        "browser": "chrome",
        "env": {
          "UNI_PLATFORM": "h5"
        },
        "define": {
          "H5-CUSTOM": true
        }
      },
      "mp-weixin-custom": {
        "title": "自定义小程序平台",
        "env": {
          "UNI_PLATFORM": "mp-weixin"
        },
        "define": {
          "MP-WEIXIN-CUSTOM": true
        }
      }
    }
  }
}
