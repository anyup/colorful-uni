{
  "easycom": {
    "^u-(.*)": "uview-ui/components/u-$1/u-$1.vue",
    "^au-(.*)": "uniui/src/components/au-$1/au-$1.vue"
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": { "navigationStyle": "custom" }
    },
    {
      "path": "pages/components/layout",
      "style": { "navigationBarTitleText": "Layout-布局" }
    },
    {
      "path": "pages/components/button",
      "style": { "navigationBarTitleText": "Button-按钮" }
    },
    {
      "path": "pages/components/loading",
      "style": { "navigationBarTitleText": "Loading-加载框" }
    },
    {
      "path": "pages/components/loader",
      "style": { "navigationBarTitleText": "Loader-加载图标" }
    },
    {
      "path": "pages/components/table",
      "style": { "navigationBarTitleText": "Table-表格" }
    },
    {
      "path": "pages/components/toast",
      "style": { "navigationBarTitleText": "Toast-提示" }
    },
    {
      "path": "pages/components/updater",
      "style": { "navigationBarTitleText": "Updater-应用更新" }
    },
    {
      "path": "pages/components/webview",
      "style": { "navigationBarTitleText": "Webview-网页试图" }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uniui",
    "navigationBarBackgroundColor": "#ffffff",
    "backgroundColor": "#ffffff",
    "navigationStyle": "default",
    "app-plus": {
      "bounce": "none",
      "softinputNavBar": "none",
      "animationType": "fade-in",
      "animationDuration": 300
    },
    "mp-alipay": {
      "titleBarColor": "#FFFFFF"
    }
  }
}
